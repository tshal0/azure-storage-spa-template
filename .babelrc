{
    "plugins": [
        "@babel/proposal-class-properties",
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "components": "./src/_components",
                    "ducks": "./src/_ducks"
                }
            }
        ]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "modules": false
            }
        ],
        [
            "@babel/preset-react",
            {
                "modules": false
            }
        ]
    ]
}